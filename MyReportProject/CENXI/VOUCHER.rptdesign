                </structure>
            </list-property>
            <xml-property name="queryText"><![CDATA[--按指令获取表内记账分录
WITH CTE_INST_ID AS (
	SELECT 
		DISTINCT 
		DECODE(SECU_INST_SETGRP_ID, NULL, SECU_INST_ID, '', SECU_INST_ID, '-1', SECU_INST_ID, '0', SECU_INST_ID, SECU_INST_SETGRP_ID) AS INST_ID, 
		PT.P_TYPE_NAME,GM.ACCOUNT_USER,GM.OPERATOR_NAME
	FROM (
		SELECT * FROM TTRD_SET_INSTRUCTION_SECU 
		UNION ALL 
		SELECT * FROM TTRD_SET_INSTRUCTION_SECU_HIS 
	) SECU, (
		SELECT * FROM TTRD_SET_INSTRUCTION 
		UNION ALL 
		SELECT * FROM TTRD_SET_INSTRUCTION_HIS 
	) M, (
		SELECT * FROM TTRD_SET_INSTRUCTION 
		UNION ALL 
		SELECT * FROM TTRD_SET_INSTRUCTION_HIS 
	) GM, TTRD_INSTRUMENT INSTR, TTRD_P_TYPE PT 
	WHERE GM.INST_ID = ? 
		AND SECU.INST_ID = M.INST_ID 
		AND M.INST_GRP_ID = GM.INST_ID 
		AND GM.H_I_CODE = INSTR.I_CODE AND GM.H_A_TYPE = INSTR.A_TYPE 
		AND GM.H_M_TYPE = INSTR.M_TYPE 
		AND INSTR.P_TYPE = PT.P_TYPE 
	UNION ALL 
	SELECT 
		DISTINCT 
		DECODE(CASH_INST_SETGRP_ID, NULL, CASH_INST_ID, '', CASH_INST_ID, '-1', CASH_INST_ID, '0', CASH_INST_ID, CASH_INST_SETGRP_ID) AS INST_ID, 
		PT.P_TYPE_NAME,GM.ACCOUNT_USER,GM.OPERATOR_NAME
	FROM (
		SELECT * FROM TTRD_SET_INSTRUCTION_CASH 
		UNION ALL 
		SELECT * FROM TTRD_SET_INSTRUCTION_CASH_HIS
	) CASH, (
		SELECT * FROM TTRD_SET_INSTRUCTION 
		UNION ALL 
		SELECT * FROM TTRD_SET_INSTRUCTION_HIS 
	) M, (
		SELECT * FROM TTRD_SET_INSTRUCTION 
		UNION ALL 
		SELECT * FROM TTRD_SET_INSTRUCTION_HIS 
	) GM, TTRD_INSTRUMENT INSTR, TTRD_P_TYPE PT 
	WHERE GM.INST_ID = ? 
		AND CASH.INST_ID = M.INST_ID 
		AND M.INST_GRP_ID = GM.INST_ID 
		AND GM.H_I_CODE = INSTR.I_CODE AND GM.H_A_TYPE = INSTR.A_TYPE 
		AND GM.H_M_TYPE = INSTR.M_TYPE 
		AND INSTR.P_TYPE = PT.P_TYPE 
)
SELECT 
	DT.*, 
	DICT.DICT_VALUE AS STEP_NAME 
FROM (
	SELECT 
		E.ENTRY_ID, 
		E.FLOW_ID, 
		E.CORE_ACCT_CODE, 
		COALESCE(E.CORE_ACCT_NAME,ACTING_ENTRY_NAME_3) AS CORE_ACCT_NAME, 
		E.SUBJ_CODE, 
		E.INNER_ACCT_SN, 
		E.DEBIT_CREDIT_FLAG, 
		E.RED_BLUE_FLAG, 
		E.STEP, 
		E.VALUE,
		I.P_TYPE_NAME,
		I.OPERATOR_NAME,
		U.USER_NAME as ACCOUNT_USER
	FROM TTRD_BOOKKEEPING_ENTRY E 
	JOIN CTE_INST_ID I 
		ON I.INST_ID = E.INST_ID 
	LEFT JOIN TTRD_ACCOUNTING_ENTRY_DEF F ON  E.SUBJ_CODE=F.ACTING_CODE
	LEFT JOIN TTRD_AUTH_USER U ON I.ACCOUNT_USER=U.USER_ID
	WHERE E.STATE = '1' AND E.GRP_FLOW_ID = '-1' 
		 
	UNION ALL 
	SELECT 
		E.ENTRY_ID, 
		E.FLOW_ID, 
		E.CORE_ACCT_CODE, 
		COALESCE(E.CORE_ACCT_NAME,ACTING_ENTRY_NAME_3) AS CORE_ACCT_NAME, 
		E.SUBJ_CODE, 
		E.INNER_ACCT_SN, 
		E.DEBIT_CREDIT_FLAG, 
		E.RED_BLUE_FLAG, 
		E.STEP, 
		E.VALUE, 
		I.P_TYPE_NAME,
		I.OPERATOR_NAME,
		U.USER_NAME as ACCOUNT_USER
	FROM TTRD_BOOKKEEPING_ENTRY_HIS E 
	JOIN CTE_INST_ID I 
		ON I.INST_ID = E.INST_ID
	LEFT JOIN TTRD_ACCOUNTING_ENTRY_DEF F ON  E.SUBJ_CODE=F.ACTING_CODE
	LEFT JOIN TTRD_AUTH_USER U ON I.ACCOUNT_USER=U.USER_ID
	WHERE E.STATE = '1' AND E.GRP_FLOW_ID = '-1' 
) DT 
LEFT JOIN TTRD_DICT_MULT_LANG DICT 
	ON DICT.DICT_SUB_TYPE = 'bkpStep' AND DICT.DICT_KEY = DT.STEP 
ORDER BY DT.STEP, DT.DEBIT_CREDIT_FLAG ]]></xml-property>
