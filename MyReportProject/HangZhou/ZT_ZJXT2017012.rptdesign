            <xml-property name="queryText"><![CDATA[with paras as (
 select decode(?,null,null,'null',null,'',null,?) para1,  --余额开始日期
        decode(?,null,null,'null',null,'',null,?) para2,  --余额结束日期
        decode(?,null,null,'null',null,'',null,?) para3,  --资产代码
        decode(?,null,null,'null',null,'',null,?) para4,  --内部证券账户
        decode(?,null,null,'null',null,'',null,?) para5   --投资类型
 from dual
)
SELECT t.accname            AS "内部证券账户",
       t.typename           AS "投资类型", 
       t.i_code             AS "资产代码",
       t.i_name             AS "资产名称",
       t.p_class            AS "资产类型",
       t.real_amount        AS "面额(元)",
       t.prft_trd           AS "买卖损益(元)",
       t.prft_fv            AS "公允价值损益(元)",
       t.prft_ir            AS "利息收入税后额(元)",
       ''                   AS "利息收入税额(元)",
       t.capital_reserve    AS "资本公积(元)",
       t.ord_id             AS "审批单号",
       t.i_fullname         AS "客户名称"
FROM 
(
     SELECT t.open_trade_id,
            t.chg_trade_id,
            t.chg_id,
            t.chg_date,
            t.secu_acct_id,
            t.inst_id,
            t.i_code,
            t.a_type,
            t.m_type,
            t.i_name,
            t.p_class,
            t.trdtypename,
            t.ord_id,
            t.typename,
            t.trd_type,
            t.accname,
            t.i_fullname,
            SUM(CASE WHEN t.inst_id = -1  THEN t.real_amount
                    ELSE t.chg_real_amount
                 END
            ) as real_amount,
            SUM(t.prft_ir) AS prft_ir,
            SUM(t.prft_trd) AS prft_trd,
            SUM(t.prft_fv) AS prft_fv,
            SUM(t.prft_ir_ai) AS prft_ir_ai,
            SUM(t.prft_ir_amrt) AS prft_ir_amrt,
            SUM(t.capital_reserve) AS capital_reserve
    FROM (
            SELECT CASE WHEN ty.description IS NULL THEN biztype 
                        ELSE ty.description 
                   END AS trdtypename,
                   ty.trd_type,
                   VP.*
            FROM vtrd_profit vp
            LEFT JOIN ttrd_trade_type ty ON vp.biztype = ty.trd_type
          ) t
    WHERE t.A_TYPE != 'SPT_DED'
    GROUP BY t.secu_acct_id,t.accname,t.typename,t.i_code,t.a_type,
         t.m_type,t.i_name,t.p_class,t.chg_id,t.chg_date,t.inst_id,
         t.ord_id,t.trdtypename,t.i_fullname,t.open_trade_id,t.chg_trade_id,t.trd_type
 ) t
 WHERE t.chg_date>=(select para1 from paras) AND t.chg_date<=(select para2 from paras) AND 
       (
          ((select para3 from paras) IS NULL OR t.i_code=(select para3 from paras)) AND 
          ((select para4 from paras) IS NULL OR t.secu_acct_id=(select para4 from paras)) AND 
          ((select para5 from paras) IS NULL OR t.trd_type=(select para4 from paras))
       )]]></xml-property>
        </oda-data-set>
