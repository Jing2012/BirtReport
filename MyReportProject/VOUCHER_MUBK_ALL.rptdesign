            <xml-property name="queryText"><![CDATA[SELECT ORG.I_FULLNAME ORG_NAME,
       T.ACCOUNT_TIME,
       T.FLOW_ID,
       T.ACCT_USER_NAME,
       U.USER_NAME,
       T.REMARK
FROM TTRD_MANUAL_BK_RECORD T
     LEFT JOIN TTRD_INSTITUTION ORG ON T.BKKPG_ORG_ID=ORG.ORG_ID
     LEFT JOIN TTRD_AUTH_USER U ON U.USER_ID=T.ACCT_REVIEW_USER
