            <xml-property name="queryText"><![CDATA[SELECT ROW_NUMBER() OVER (ORDER BY T.CURRENCY,T.DEBIT_CREDIT_FLAG) RUM,
       T.SUBJ_CODE,
       D.ACTING_ENTRY_NAME_3 SUBJ_NAME,
       T.CURRENCY,
       DECODE(T.DEBIT_CREDIT_FLAG,'1','借','贷') DEBIT_CREDIT,
       T.VALUE
FROM (SELECT * FROM TTRD_BOOKKEEPING_ENTRY UNION ALL SELECT * FROM TTRD_BOOKKEEPING_ENTRY_HIS) T
     INNER JOIN TTRD_MANUAL_BK_RECORD R ON R.FLOW_ID = T.FLOW_ID 
     LEFT JOIN TTRD_ACCOUNTING_ENTRY_DEF D ON T.SUBJ_CODE = D.ACTING_CODE
WHERE R.RECORD_ID=?]]></xml-property>
