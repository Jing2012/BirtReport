            <xml-property name="queryText"><![CDATA[--当日业务提示
WITH FULDATE AS(
 SELECT CAL_DAY FROM (
   SELECT T.*,ROW_NUMBER() OVER (ORDER BY CAL_DAY) FNO 
   FROM TCALENDAR_DATES T 
   WHERE CAL_CODE='CHINA_IB' 
   AND CAL_DAY>=(SELECT CURR_DATE FROM TTRD_CURRDATE) 
   AND CAL_FLAG<>0 
   ORDER BY CAL_DAY )
 WHERE FNO=2
)
SELECT FTYPE,DECODE(FNO,1,FLAG,NULL) FLAG,FNO,I_CODE,I_NAME,ORDAMOUNT FROM(

  --同业投资今日投资
  SELECT '同业投资' FTYPE,'今日投资' FLAG, ROWNUM FNO,
  CASE WHEN T.TRDTYPE='0121100' THEN T.ORD_ID ELSE T.I_CODE END AS I_CODE,
  CASE WHEN T.TRDTYPE='0121100' THEN I.P_CLASS ELSE I.I_NAME END AS I_NAME,
  T.ORDAMOUNT,1 SORTER
  FROM TTRD_OTC_TRADE T 
  INNER JOIN TTRD_INSTRUMENT I ON T.I_CODE=I.I_CODE AND T.A_TYPE=I.A_TYPE
  LEFT JOIN TTRD_OTC_ORDER O ON T.ORD_ID=O.ORD_ID
  WHERE (T.TRDTYPE IN('0100000','0100400','0702080')
  OR T.TRDTYPE = '0121100' AND I.P_CLASS='存放同业活期')
  AND COALESCE(O.ORDDATE,T.ORDDATE)>=(SELECT CURR_DATE FROM TTRD_CURRDATE)
  AND COALESCE(O.ORDDATE,T.ORDDATE)<=?
  AND T.ORDSTATUS IN (-1,0,3,4)
UNION ALL
  --同业投资今日起息
  SELECT '同业投资' FTYPE,'今日起息' FLAG, ROWNUM FNO,
  CASE WHEN T.TRDTYPE='0121100' THEN T.ORD_ID ELSE T.I_CODE END AS I_CODE,
  CASE WHEN T.TRDTYPE='0121100' THEN I.P_CLASS ELSE I.I_NAME END AS I_NAME,
  ABS(DECODE(T.ORDSTATUS,-1,T.ORDAMOUNT,C.AMOUNT)) ORDAMOUNT,2
  FROM TTRD_OTC_TRADE T 
  INNER JOIN TTRD_INSTRUMENT I ON T.I_CODE=I.I_CODE AND T.A_TYPE=I.A_TYPE
  LEFT JOIN TTRD_SET_INSTRUCTION S ON T.INTORDID=S.TRADE_ID
  LEFT JOIN TTRD_SET_INSTRUCTION_CASH C ON S.INST_ID=C.INST_ID
  WHERE (T.TRDTYPE IN ('0100000','0100400','0702080')
  OR T.TRDTYPE = '0121100' AND I.P_CLASS='存放同业活期')
  AND T.SETDATE>=(SELECT CURR_DATE FROM TTRD_CURRDATE)
  AND T.SETDATE<=?
  AND S.TRD_TYPE =T.TRDTYPE
  AND (T.ORDSTATUS IN(3,4) AND S.STATE<>99999)
UNION ALL
  --同业投资今日到期
  SELECT '同业投资' FTYPE,'今日到期' FLAG,ROWNUM FNO,I_CODE,I_NAME,ORDAMOUNT,3 FROM(
  SELECT 
  CASE WHEN S.TRD_TYPE='0121301' THEN S.ORDER_ID ELSE I.I_CODE END AS I_CODE,
  CASE WHEN S.TRD_TYPE='0121301' THEN I.P_CLASS ELSE I.I_NAME END AS I_NAME,
  ABS(C.AMOUNT) ORDAMOUNT,S.REAL_SET_DATE MTR_DATE
  FROM TTRD_SET_INSTRUCTION S 
  INNER JOIN TTRD_INSTRUMENT I ON S.H_I_CODE=I.I_CODE AND S.H_A_TYPE=I.A_TYPE
  INNER JOIN TTRD_SET_INSTRUCTION_CASH C ON S.INST_ID=C.INST_ID
  WHERE (S.TRD_TYPE IN('0100301') 
  OR S.TRD_TYPE = '0121301' AND I.P_CLASS='存放同业活期')
  AND S.REAL_SET_DATE>=(SELECT CURR_DATE FROM TTRD_CURRDATE)
  AND S.REAL_SET_DATE<=?
  AND S.STATE<>99999
  UNION ALL
  SELECT T.I_CODE,T.I_NAME,A.REAL_AMOUNT+A.AI ORDAMOUNT,MTR_DATE
  FROM (
    SELECT 
    CASE WHEN T.TRDTYPE='0121100' THEN T.ORD_ID ELSE T.I_CODE END AS I_CODE,
    CASE WHEN T.TRDTYPE='0121100' THEN I.P_CLASS ELSE I.I_NAME END AS I_NAME,
    T.INTORDID,CHG.ACCTG_OBJ_ID,I.MTR_DATE,T.ORDAMOUNT
    FROM TTRD_OTC_TRADE T 
    INNER JOIN TTRD_INSTRUMENT I ON T.I_CODE=I.I_CODE AND T.A_TYPE=I.A_TYPE
    INNER JOIN (SELECT * FROM TTRD_SET_INSTRUCTION UNION ALL 
                SELECT * FROM TTRD_SET_INSTRUCTION_HIS) INST ON T.INTORDID=INST.TRADE_ID
    INNER JOIN (SELECT * FROM TTRD_SET_INSTRUCTION_SECU UNION ALL 
                SELECT * FROM TTRD_SET_INSTRUCTION_SECU_HIS) SECU ON INST.INST_ID=SECU.INST_ID
    INNER JOIN TTRD_ACCOUNTING_SECU_CHG_HIS CHG ON CHG.INST_ID=SECU.SECU_INST_ID
    WHERE (T.TRDTYPE IN('0100000','0100400')
    OR T.TRDTYPE = '0121100' AND I.P_CLASS='存放同业活期')
    AND I.MTR_DATE>(SELECT CAL_DAY FROM FULDATE)
    AND I.MTR_DATE<=?
    AND T.ORDSTATUS = 4 
    AND T.INSSTATUS =999 
    AND INST.STATE<>99999
    UNION --不能用ALL，如果有多条核算变动一条核算余额也会关联出来多条，不用ALL有个去重作用
    SELECT 
    CASE WHEN T.TRDTYPE='0121100' THEN T.ORD_ID ELSE T.I_CODE END AS I_CODE,
    CASE WHEN T.TRDTYPE='0121100' THEN I.P_CLASS ELSE I.I_NAME END AS I_NAME,
    T.INTORDID,CHG.ACCTG_OBJ_ID,I.MTR_DATE,T.ORDAMOUNT
    FROM TTRD_OTC_TRADE T 
    INNER JOIN TTRD_INSTRUMENT I ON T.I_CODE=I.I_CODE AND T.A_TYPE=I.A_TYPE
    INNER JOIN (SELECT * FROM TTRD_SET_INSTRUCTION UNION ALL 
                SELECT * FROM TTRD_SET_INSTRUCTION_HIS) INST ON T.INTORDID=INST.TRADE_ID
    INNER JOIN (SELECT * FROM TTRD_SET_INSTRUCTION_CASH UNION ALL 
                SELECT * FROM TTRD_SET_INSTRUCTION_CASH_HIS) CASH ON INST.INST_ID=CASH.INST_ID
    INNER JOIN TTRD_ACCOUNTING_SECU_CHG_HIS CHG ON CHG.INST_ID=CASH.CASH_INST_ID
    WHERE (T.TRDTYPE IN('0100000','0100400')
    OR T.TRDTYPE = '0121100' AND I.P_CLASS='存放同业活期')
    AND I.MTR_DATE>(SELECT CAL_DAY FROM FULDATE)
    AND I.MTR_DATE<=?
    AND T.ORDSTATUS = 4 
    AND T.INSSTATUS =999 
    AND INST.STATE<>99999
    ) T 
  LEFT JOIN TTRD_ACCOUNTING_SECU_OBJ A ON T.ACCTG_OBJ_ID=A.OBJ_ID) 

  UNION ALL
  --同业投融资今日投资
  SELECT '同业投融资' FTYPE,'今日投资' FLAG, ROWNUM FNO,T.ORD_ID I_CODE,PT.P_TYPE_NAME,T.ORDAMOUNT,4
  FROM TTRD_OTC_TRADE T 
  INNER JOIN TTRD_INSTRUMENT I ON T.I_CODE=I.I_CODE AND T.A_TYPE=I.A_TYPE
  LEFT JOIN TTRD_OTC_ORDER O ON T.ORD_ID=O.ORD_ID
  LEFT JOIN TTRD_P_TYPE PT ON PT.P_TYPE=I.P_TYPE
  WHERE (T.TRDTYPE IN('0134101','0125101','0125100')
  OR T.TRDTYPE = '0121100' AND I.P_CLASS='存放同业定期')
  AND COALESCE(O.ORDDATE,T.ORDDATE)>=(SELECT CURR_DATE FROM TTRD_CURRDATE)
  AND COALESCE(O.ORDDATE,T.ORDDATE)<=?
  AND T.ORDSTATUS IN (-1,3,4)
UNION ALL
  --同业投融资今日起息
  SELECT '同业投融资' FTYPE,'今日起息' FLAG, ROWNUM FNO,T.ORD_ID I_CODE,PT.P_TYPE_NAME,
  ABS(DECODE(T.ORDSTATUS,-1,T.ORDAMOUNT,C.AMOUNT)) ORDAMOUNT,5
  FROM TTRD_OTC_TRADE T 
  INNER JOIN TTRD_INSTRUMENT I ON T.I_CODE=I.I_CODE AND T.A_TYPE=I.A_TYPE
  LEFT JOIN TTRD_SET_INSTRUCTION S ON T.INTORDID=S.TRADE_ID
  LEFT JOIN TTRD_SET_INSTRUCTION_CASH C ON S.INST_ID=C.INST_ID
  LEFT JOIN TTRD_P_TYPE PT ON PT.P_TYPE=I.P_TYPE
  WHERE (T.TRDTYPE IN('0134101','0125101','0125100') 
  OR T.TRDTYPE = '0121100' AND I.P_CLASS='存放同业定期' )
  AND T.SETDATE>=(SELECT CURR_DATE FROM TTRD_CURRDATE)
  AND T.SETDATE<=?
  AND S.TRD_TYPE =T.TRDTYPE
  AND (T.ORDSTATUS=-1 OR T.ORDSTATUS IN(3,4) AND S.STATE<>99999)
UNION ALL
  --同业投融资今日到期 
  SELECT '同业投融资' FTYPE,'今日到期' FLAG,ROWNUM FNO,I_CODE,I_NAME,ORDAMOUNT,6 FROM(
  SELECT S.ORDER_ID I_CODE,I.P_CLASS I_NAME, ABS(C.AMOUNT) ORDAMOUNT,S.REAL_SET_DATE MTR_DATE
  FROM TTRD_SET_INSTRUCTION S 
  INNER JOIN TTRD_INSTRUMENT I ON S.H_I_CODE=I.I_CODE AND S.H_A_TYPE=I.A_TYPE
  INNER JOIN TTRD_SET_INSTRUCTION_CASH C ON S.INST_ID=C.INST_ID
  WHERE (S.TRD_TYPE IN('0134101','0125101','0125100')
  OR S.TRD_TYPE = '0121301' AND I.P_CLASS='存放同业定期' )
  AND S.REAL_SET_DATE>=(SELECT CURR_DATE FROM TTRD_CURRDATE)
  AND S.REAL_SET_DATE<=?
  AND S.STATE<>99999
  UNION ALL
  SELECT T.I_CODE,T.I_NAME,A.REAL_AMOUNT+A.AI ORDAMOUNT,MTR_DATE
  FROM (
    SELECT T.ORD_ID I_CODE,I.P_CLASS I_NAME,
    T.INTORDID,CHG.ACCTG_OBJ_ID,I.MTR_DATE,T.ORDAMOUNT
    FROM TTRD_OTC_TRADE T 
    INNER JOIN TTRD_INSTRUMENT I ON T.I_CODE=I.I_CODE AND T.A_TYPE=I.A_TYPE
    INNER JOIN (SELECT * FROM TTRD_SET_INSTRUCTION UNION ALL 
                SELECT * FROM TTRD_SET_INSTRUCTION_HIS) INST ON T.INTORDID=INST.TRADE_ID
    INNER JOIN (SELECT * FROM TTRD_SET_INSTRUCTION_SECU UNION ALL 
                SELECT * FROM TTRD_SET_INSTRUCTION_SECU_HIS) SECU ON INST.INST_ID=SECU.INST_ID
    INNER JOIN TTRD_ACCOUNTING_SECU_CHG_HIS CHG ON CHG.INST_ID=SECU.SECU_INST_ID
    WHERE (T.TRDTYPE IN('0100000','0100400')
    OR T.TRDTYPE = '0121100' AND I.P_CLASS='存放同业定期')
    AND I.MTR_DATE>(SELECT CAL_DAY FROM FULDATE)
    AND I.MTR_DATE<=?
    AND T.ORDSTATUS = 4 
    AND T.INSSTATUS =999 
    AND INST.STATE<>99999
    UNION --不能用ALL，如果有多条核算变动一条核算余额也会关联出来多条，不用ALL有个去重作用
    SELECT T.ORD_ID I_CODE,I.P_CLASS I_NAME,
    T.INTORDID,CHG.ACCTG_OBJ_ID,I.MTR_DATE,T.ORDAMOUNT
    FROM TTRD_OTC_TRADE T 
    INNER JOIN TTRD_INSTRUMENT I ON T.I_CODE=I.I_CODE AND T.A_TYPE=I.A_TYPE
    INNER JOIN (SELECT * FROM TTRD_SET_INSTRUCTION UNION ALL 
                SELECT * FROM TTRD_SET_INSTRUCTION_HIS) INST ON T.INTORDID=INST.TRADE_ID
    INNER JOIN (SELECT * FROM TTRD_SET_INSTRUCTION_CASH UNION ALL 
                SELECT * FROM TTRD_SET_INSTRUCTION_CASH_HIS) CASH ON INST.INST_ID=CASH.INST_ID
    INNER JOIN TTRD_ACCOUNTING_SECU_CHG_HIS CHG ON CHG.INST_ID=CASH.CASH_INST_ID
    WHERE (T.TRDTYPE IN('0100000','0100400')
    OR T.TRDTYPE = '0121100' AND I.P_CLASS='存放同业定期')
    AND I.MTR_DATE>(SELECT CAL_DAY FROM FULDATE)
    AND I.MTR_DATE<=?
    AND T.ORDSTATUS = 4 
    AND T.INSSTATUS =999 
    AND INST.STATE<>99999
    ) T 
  LEFT JOIN TTRD_ACCOUNTING_SECU_OBJ A ON T.ACCTG_OBJ_ID=A.OBJ_ID) 

UNION ALL
  --存单发行今日投资
  SELECT '同业投融资' FTYPE,'今日存单投资' FLAG, ROWNUM FNO,T.I_CODE,TE.DESCRIPTION I_NAME,T.ORDAMOUNT,7
  FROM TTRD_OTC_TRADE T 
  INNER JOIN TTRD_INSTRUMENT I ON T.I_CODE=I.I_CODE AND T.A_TYPE=I.A_TYPE
  LEFT JOIN TTRD_OTC_ORDER O ON T.ORD_ID=O.ORD_ID
  LEFT JOIN TTRD_TRADE_TYPE TE ON T.TRDTYPE = TE.TRD_TYPE
  WHERE T.TRDTYPE ='0000081' 
  AND COALESCE(O.ORDDATE,T.ORDDATE)>=(SELECT CURR_DATE FROM TTRD_CURRDATE)
  AND COALESCE(O.ORDDATE,T.ORDDATE)<=?
  AND (T.ORDSTATUS =-1 OR T.ORDSTATUS IN(3,4) AND T.REF_TYPE=7 )
UNION ALL
  --存单发行今日起息
  SELECT '同业投融资' FTYPE,'今日存单起息' FLAG, ROWNUM FNO,T.I_CODE,TE.DESCRIPTION I_NAME,
  ABS(DECODE(T.ORDSTATUS,-1,T.ORDAMOUNT,C.AMOUNT)) ORDAMOUNT,8
  FROM TTRD_OTC_TRADE T 
  INNER JOIN TTRD_INSTRUMENT I ON T.I_CODE=I.I_CODE AND T.A_TYPE=I.A_TYPE
  LEFT JOIN TTRD_SET_INSTRUCTION S ON T.INTORDID=S.TRADE_ID 
  LEFT JOIN TTRD_SET_INSTRUCTION_CASH C ON S.INST_ID=C.INST_ID
  LEFT JOIN TTRD_TRADE_TYPE TE ON T.TRDTYPE = TE.TRD_TYPE
  WHERE T.TRDTYPE ='0000081' 
  AND T.SETDATE>=(SELECT CURR_DATE FROM TTRD_CURRDATE)
  AND T.SETDATE<=?
  AND S.TRD_TYPE = '0000081'
  AND (T.ORDSTATUS =-1 OR T.ORDSTATUS IN (3,4) AND T.REF_TYPE=7 AND S.STATE<>99999)
UNION ALL
  --存单发行今日到期
  SELECT '同业投融资' FTYPE,'今日存单到期' FLAG, ROWNUM FNO,I_CODE,I_NAME, ORDAMOUNT,9
  FROM (
  SELECT T.I_CODE,TE.DESCRIPTION I_NAME, ABS(C.AMOUNT) ORDAMOUNT,S.REAL_SET_DATE MTR_DATE
  FROM TTRD_SET_INSTRUCTION S 
  INNER JOIN TTRD_OTC_TRADE T ON S.TRADE_ID=T.INTORDID 
  INNER JOIN TTRD_INSTRUMENT I ON T.I_CODE=I.I_CODE AND S.H_A_TYPE=I.A_TYPE
  LEFT JOIN TTRD_SET_INSTRUCTION_CASH C ON S.INST_ID=C.INST_ID
  LEFT JOIN TTRD_TRADE_TYPE TE ON T.TRDTYPE = TE.TRD_TYPE
  WHERE S.TRD_TYPE = '0000300' AND T.TRDTYPE='0000081' AND T.REF_TYPE=7
  AND S.REAL_SET_DATE>=(SELECT CURR_DATE FROM TTRD_CURRDATE)
  AND S.REAL_SET_DATE<=?
  AND S.STATE<>99999
  UNION ALL
  SELECT T.I_CODE,T.I_NAME,A.REAL_AMOUNT+A.AI ORDAMOUNT,MTR_DATE
  FROM (
  	SELECT T.ORD_ID I_CODE,TE.DESCRIPTION I_NAME,
    T.INTORDID,CHG.ACCTG_OBJ_ID,I.MTR_DATE,T.ORDAMOUNT
    FROM TTRD_OTC_TRADE T 
    INNER JOIN TTRD_INSTRUMENT I ON T.I_CODE=I.I_CODE AND T.A_TYPE=I.A_TYPE
    INNER JOIN (SELECT * FROM TTRD_SET_INSTRUCTION UNION ALL 
                SELECT * FROM TTRD_SET_INSTRUCTION_HIS) INST ON T.INTORDID=INST.TRADE_ID
    INNER JOIN (SELECT * FROM TTRD_SET_INSTRUCTION_SECU UNION ALL 
                SELECT * FROM TTRD_SET_INSTRUCTION_SECU_HIS) SECU ON INST.INST_ID=SECU.INST_ID
    INNER JOIN TTRD_ACCOUNTING_SECU_CHG_HIS CHG ON CHG.INST_ID=SECU.SECU_INST_ID
    LEFT JOIN TTRD_TRADE_TYPE TE ON T.TRDTYPE = TE.TRD_TYPE
    WHERE T.TRDTYPE ='0000081'
    AND I.MTR_DATE>(SELECT CAL_DAY FROM FULDATE)
    AND I.MTR_DATE<=?
    AND T.ORDSTATUS = 4 
    AND T.INSSTATUS =999 
    AND INST.STATE<>99999
    UNION --不能用ALL，如果有多条核算变动一条核算余额也会关联出来多条，不用ALL有个去重作用  
    SELECT T.ORD_ID I_CODE,TE.DESCRIPTION I_NAME,
    T.INTORDID,CHG.ACCTG_OBJ_ID,I.MTR_DATE,T.ORDAMOUNT
    FROM TTRD_OTC_TRADE T 
    INNER JOIN TTRD_INSTRUMENT I ON T.I_CODE=I.I_CODE AND T.A_TYPE=I.A_TYPE
    INNER JOIN (SELECT * FROM TTRD_SET_INSTRUCTION UNION ALL 
                SELECT * FROM TTRD_SET_INSTRUCTION_HIS) INST ON T.INTORDID=INST.TRADE_ID
    INNER JOIN (SELECT * FROM TTRD_SET_INSTRUCTION_CASH UNION ALL 
                SELECT * FROM TTRD_SET_INSTRUCTION_CASH_HIS) CASH ON INST.INST_ID=CASH.INST_ID
    INNER JOIN TTRD_ACCOUNTING_SECU_CHG_HIS CHG ON CHG.INST_ID=CASH.CASH_INST_ID
    LEFT JOIN TTRD_TRADE_TYPE TE ON T.TRDTYPE = TE.TRD_TYPE
    WHERE T.TRDTYPE='0000081'
    AND I.MTR_DATE>(SELECT CAL_DAY FROM FULDATE) 
    AND I.MTR_DATE<=?
    AND T.ORDSTATUS = 4 
    AND T.INSSTATUS =999 
    AND INST.STATE<>99999
    ) T 
  LEFT JOIN TTRD_ACCOUNTING_SECU_OBJ A ON T.ACCTG_OBJ_ID=A.OBJ_ID) 
  )
ORDER BY SORTER,FNO
]]></xml-property>
