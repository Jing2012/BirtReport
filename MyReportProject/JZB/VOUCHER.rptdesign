                    <property name="nativeName">DEBIT_VALUE</property>
                    <property name="dataType">decimal</property>
                </structure>
                <structure>
                    <property name="position">7</property>
                    <property name="name">CREDIT_VALUE</property>
                    <property name="nativeName">CREDIT_VALUE</property>
                    <property name="dataType">decimal</property>
                </structure>
                <structure>
                    <property name="position">8</property>
                    <property name="name">DEBIT_CREDIT_FLAG</property>
                    <property name="nativeName">DEBIT_CREDIT_FLAG</property>
                    <property name="dataType">string</property>
                </structure>
                <structure>
                    <property name="position">9</property>
                    <property name="name">RED_BLUE_FLAG</property>
                    <property name="nativeName">RED_BLUE_FLAG</property>
                    <property name="dataType">string</property>
                </structure>
                <structure>
                    <property name="position">10</property>
                    <property name="name">STEP</property>
                    <property name="nativeName">STEP</property>
                    <property name="dataType">string</property>
                </structure>
            </list-property>
            <xml-property name="queryText"><![CDATA[WITH CTE_INST_ID AS (
  SELECT 
    DISTINCT 
    DECODE(SECU_INST_SETGRP_ID, NULL, SECU_INST_ID, '', SECU_INST_ID, '-1', SECU_INST_ID, '0', SECU_INST_ID, SECU_INST_SETGRP_ID) AS INST_ID,
    GM.INST_ID GRPID 
  FROM (
    SELECT * FROM TTRD_SET_INSTRUCTION_SECU 
    UNION ALL 
    SELECT * FROM TTRD_SET_INSTRUCTION_SECU_HIS 
  ) SECU, (
    SELECT * FROM TTRD_SET_INSTRUCTION 
    UNION ALL 
    SELECT * FROM TTRD_SET_INSTRUCTION_HIS 
  ) M, (
    SELECT * FROM TTRD_SET_INSTRUCTION 
    UNION ALL 
    SELECT * FROM TTRD_SET_INSTRUCTION_HIS 
  ) GM 
  WHERE GM.INST_ID = ?
    AND SECU.INST_ID = M.INST_ID 
    AND M.INST_GRP_ID = GM.INST_ID 
  UNION ALL 
  SELECT 
    DISTINCT 
    DECODE(CASH_INST_SETGRP_ID, NULL, CASH_INST_ID, '', CASH_INST_ID, '-1', CASH_INST_ID, '0', CASH_INST_ID, CASH_INST_SETGRP_ID) AS INST_ID,
    GM.INST_ID GRPID  
  FROM (
    SELECT * FROM TTRD_SET_INSTRUCTION_CASH 
    UNION ALL 
    SELECT * FROM TTRD_SET_INSTRUCTION_CASH_HIS
  ) CASH, (
    SELECT * FROM TTRD_SET_INSTRUCTION 
    UNION ALL 
    SELECT * FROM TTRD_SET_INSTRUCTION_HIS 
  ) M, (
    SELECT * FROM TTRD_SET_INSTRUCTION 
    UNION ALL 
    SELECT * FROM TTRD_SET_INSTRUCTION_HIS 
  ) GM 
  WHERE GM.INST_ID = ? 
    AND CASH.INST_ID = M.INST_ID 
    AND M.INST_GRP_ID = GM.INST_ID 
), ENTRYDS AS(
  SELECT
    D.* 
  FROM (
    SELECT 
      E.FLOW_ID, 
      E.ENTRY_DATE, 
      E.CORE_ACCT_CODE, 
      E.CORE_ACCT_NAME, 
      E.SUBJ_CODE, 
      CASE WHEN E.DEBIT_CREDIT_FLAG = '1' OR E.DEBIT_CREDIT_FLAG = '3' THEN 
        E.VALUE
      ELSE 0 END AS DEBIT_VALUE, 
      CASE WHEN E.DEBIT_CREDIT_FLAG = '2' OR E.DEBIT_CREDIT_FLAG = '4' THEN 
        E.VALUE
      ELSE 0 END AS CREDIT_VALUE, 
      E.DEBIT_CREDIT_FLAG, 
      E.RED_BLUE_FLAG, 
      E.STEP, 
      INST.GRPID
    FROM TTRD_BOOKKEEPING_ENTRY E 
    JOIN CTE_INST_ID INST 
      ON INST.INST_ID = E.INST_ID 
    WHERE E.STATE = '1' AND E.GRP_FLOW_ID = '-1' 

    UNION ALL
    
    SELECT 
      E.FLOW_ID, 
      E.ENTRY_DATE, 
      E.CORE_ACCT_CODE, 
      E.CORE_ACCT_NAME, 
      E.SUBJ_CODE, 
      CASE WHEN E.DEBIT_CREDIT_FLAG = '1' OR E.DEBIT_CREDIT_FLAG = '3' THEN 
        E.VALUE
      ELSE 0 END AS DEBIT_VALUE, 
      CASE WHEN E.DEBIT_CREDIT_FLAG = '2' OR E.DEBIT_CREDIT_FLAG = '4' THEN 
        E.VALUE
      ELSE 0 END AS CREDIT_VALUE, 
      E.DEBIT_CREDIT_FLAG, 
      E.RED_BLUE_FLAG, 
      E.STEP,
      INST.GRPID 
    FROM TTRD_BOOKKEEPING_ENTRY_HIS E
    JOIN CTE_INST_ID INST 
      ON INST.INST_ID = E.INST_ID 
    WHERE E.STATE = '1' AND E.GRP_FLOW_ID = '-1' 
  ) D 
),CHGFLOWID AS(
  SELECT DISTINCT FLOW_ID,GRPID FROM ENTRYDS E WHERE EXISTS (SELECT 1 FROM ENTRYDS WHERE GRPID=E.GRPID AND STEP=0) AND STEP=4 
) SELECT COALESCE(C.FLOW_ID,E.FLOW_ID) FLOW_ID,
    E.ENTRY_DATE,E.CORE_ACCT_CODE,E.CORE_ACCT_NAME,E.SUBJ_CODE,
    E.DEBIT_VALUE,E.CREDIT_VALUE,E.DEBIT_CREDIT_FLAG,E.RED_BLUE_FLAG,STEP 
  FROM ENTRYDS E LEFT JOIN CHGFLOWID C ON E.GRPID=C.GRPID  
  ORDER BY E.FLOW_ID, E.STEP, E.DEBIT_CREDIT_FLAG]]></xml-property>
