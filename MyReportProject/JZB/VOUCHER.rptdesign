            <xml-property name="queryText"><![CDATA[WITH CTE_INST_ID AS (
  SELECT 
    DISTINCT 
    DECODE(SECU_INST_SETGRP_ID, NULL, SECU_INST_ID, '', SECU_INST_ID, '-1', SECU_INST_ID, '0', SECU_INST_ID, SECU_INST_SETGRP_ID) AS INST_ID, 
    GM.INST_ID GRPID,
    GM.TRD_TYPE, 
    GM.ACCOUNT_USER, 
    GM.REF_SECU_INST_ID, 
    GM.REF_CASH_INST_ID, 
    GM.MEMO,
    GM.INST_GRP_ID 
  FROM (
    SELECT * FROM TTRD_SET_INSTRUCTION_SECU 
    UNION ALL 
    SELECT * FROM TTRD_SET_INSTRUCTION_SECU_HIS 
  ) SECU, (
    SELECT * FROM TTRD_SET_INSTRUCTION 
    UNION ALL 
    SELECT * FROM TTRD_SET_INSTRUCTION_HIS 
  ) M, (
    SELECT * FROM TTRD_SET_INSTRUCTION 
    UNION ALL 
    SELECT * FROM TTRD_SET_INSTRUCTION_HIS 
  ) GM 
  WHERE GM.INST_ID = ?
    AND SECU.INST_ID = M.INST_ID  
    AND M.INST_GRP_ID = GM.INST_ID 
  UNION ALL 
  SELECT 
    DISTINCT 
    DECODE(CASH_INST_SETGRP_ID, NULL, CASH_INST_ID, '', CASH_INST_ID, '-1', CASH_INST_ID, '0', CASH_INST_ID, CASH_INST_SETGRP_ID) AS INST_ID, 
    GM.INST_ID GRPID,
    GM.TRD_TYPE, 
    GM.ACCOUNT_USER, 
    GM.REF_SECU_INST_ID, 
    GM.REF_CASH_INST_ID, 
    GM.MEMO,
    GM.INST_GRP_ID 
  FROM (
    SELECT * FROM TTRD_SET_INSTRUCTION_CASH 
    UNION ALL 
    SELECT * FROM TTRD_SET_INSTRUCTION_CASH_HIS
  ) CASH, (
    SELECT * FROM TTRD_SET_INSTRUCTION 
    UNION ALL 
    SELECT * FROM TTRD_SET_INSTRUCTION_HIS 
  ) M, (
    SELECT * FROM TTRD_SET_INSTRUCTION 
    UNION ALL 
    SELECT * FROM TTRD_SET_INSTRUCTION_HIS 
  ) GM 
  WHERE GM.INST_ID = ?
    AND CASH.INST_ID = M.INST_ID 
    AND M.INST_GRP_ID = GM.INST_ID 
),INFODS AS(
  SELECT 
    DISTINCT 
    D.FLOW_ID, 
    SUBSTR(D.FLOW_ID, -4) AS CARD_ID, 
    D.ENTRY_DATE, 
    D.BKKPG_ORG_ID, 
    D.CURRENCY, 
    D.REF_SECU_INST_ID, 
    D.REF_CASH_INST_ID, 
    D.MEMO, 
    D.STEP,
    D.GRPID,
    I.I_FULLNAME AS BKKPG_ORG_NAME, 
    TP.DESCRIPTION AS TRD_TYPENAME, 
    U.USER_NAME AS ACCT_USER_NAME, 
    LG.SEND_TIME 
  FROM (
    SELECT 
      E.FLOW_ID, 
      E.ENTRY_DATE, 
      E.BKKPG_ORG_ID, 
      E.INST_ID, 
      E.CURRENCY, 
      E.STEP,
      INST.GRPID,
      INST.TRD_TYPE, 
      INST.ACCOUNT_USER,
      INST.REF_SECU_INST_ID, 
      INST.REF_CASH_INST_ID, 
      INST.MEMO, 
      INST.INST_GRP_ID 
    FROM TTRD_BOOKKEEPING_ENTRY E 
    JOIN CTE_INST_ID INST 
      ON INST.INST_ID = E.INST_ID  
    WHERE E.STATE = '1' AND E.GRP_FLOW_ID = '-1' 
    
    UNION ALL
    SELECT 
      E.FLOW_ID, 
      E.ENTRY_DATE, 
      E.BKKPG_ORG_ID, 
      E.INST_ID, 
      E.CURRENCY, 
      E.STEP,
      INST.GRPID,
      INST.TRD_TYPE, 
      INST.ACCOUNT_USER,
      INST.REF_SECU_INST_ID, 
      INST.REF_CASH_INST_ID, 
      INST.MEMO, 
      INST.INST_GRP_ID 
    FROM TTRD_BOOKKEEPING_ENTRY_HIS E 
    JOIN CTE_INST_ID INST 
      ON INST.INST_ID = E.INST_ID 
    WHERE E.STATE = '1' AND E.GRP_FLOW_ID = '-1' 
      AND (E.ESTD_OR_REAL = 'R' OR (E.ESTD_OR_REAL = 'E' AND E.CHG_TYPE = '1')) 
  ) D 
  LEFT JOIN TTRD_INSTITUTION I 
    ON I.ORG_ID = D.BKKPG_ORG_ID 
  LEFT JOIN TTRD_AUTH_USER U
    ON U.USER_ID = D.ACCOUNT_USER 
  LEFT JOIN TTRD_TRADE_TYPE TP 
    ON TP.TRD_TYPE = D.TRD_TYPE 
  LEFT JOIN (
    SELECT 
      INST_ID, MAX(CREATETIME) AS SEND_TIME 
    FROM TTRD_SETTLE_LOG 
    WHERE INST_ID = ? 
    GROUP BY INST_ID 
  ) LG 
  ON LG.INST_ID = D.INST_GRP_ID
),
SELECT4 AS(
  SELECT * FROM INFODS O WHERE EXISTS(SELECT 1 FROM INFODS WHERE GRPID=O.GRPID AND STEP=0) AND STEP=4
)
SELECT * FROM INFODS WHERE GRPID NOT IN (SELECT GRPID FROM SELECT4)
UNION ALL 
SELECT * FROM SELECT4]]></xml-property>
