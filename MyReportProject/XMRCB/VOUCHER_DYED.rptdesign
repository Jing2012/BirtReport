            <xml-property name="queryText"><![CDATA[--CHG_ID关联记账变动，找到ACCTG_OBJ_ID对应的核算对象
SELECT 
	ROW_NUMBER() OVER(ORDER BY INNER_ACCT_SN, P_TYPE_GRP) AS RN, 
	D.*
FROM (
	SELECT 
		DISTINCT 
		INNER_ACCT_SN, 
		CASE WHEN P_TYPE = '0121' THEN 
			'0121' 
		WHEN P_TYPE = '0134' AND (T_TYPE = '银行系统内-省联社' OR T_TYPE = '银行系统内-其他联社') THEN 
			'0134_1' 
		WHEN P_TYPE = '0134' AND (T_TYPE = '银行系统外') THEN 
			'0134_2'
		WHEN P_TYPE = '0134' AND T_TYPE <> '银行系统内-省联社' AND T_TYPE <> '银行系统内-其他联社' AND T_TYPE <>'银行系统外' THEN 
			'0134_3'
		WHEN P_TYPE = '0100' AND P_CLASS = '保本理财' THEN 
			'0100_1'
		WHEN P_TYPE = '0100' AND P_CLASS = '买入返售其他金融资产' THEN 
			'0100_2'
		WHEN P_TYPE = '0100' AND P_CLASS <> '保本理财' AND P_CLASS <> '买入返售其他金融资产' THEN 
			'0100_3'
		WHEN P_TYPE = '0000' AND ACCTYPE_FLAG = 0 THEN 
			'0000_1'
		WHEN P_TYPE = '0000' AND ACCTYPE_FLAG > 0 THEN 
			'0000_2' 
		END AS P_TYPE_GRP 
	FROM (
		SELECT 
			E.INNER_ACCT_SN, 
			INSTM.P_TYPE, 
			PT.P_TYPE_NAME, 
			INSTM.P_CLASS, 
			INSTR((SELECT ','||P_VALUE||',' FROM TPARA WHERE P_CODE='BONDRIGHTACCOUNTYPES'),','||ACCS.ACTING_TYPE||',') AS ACCTYPE_FLAG, 
			SUBSTR(INSTU.T_PATH, 0, INSTR(INSTU.T_PATH, '.') - 1) AS T_TYPE 
		FROM TTRD_BOOKKEEPING_ENTRY_HIS E 
		JOIN TTRD_SETTLE_PRINT_LOG L 
			ON L.FLOW_ID = E.FLOW_ID 
		JOIN TTRD_BOOKKEEPING_SECU_CHG_HIS C 
			ON C.CHG_ID = E.CHG_ID 
		JOIN TTRD_ACCOUNTING_SECU_OBJ_HIS O 
			ON O.OBJ_ID = C.ACCTG_OBJ_ID 
			AND O.BEG_DATE = C.CHG_DATE 
		JOIN TTRD_INSTRUMENT INSTM 
			ON INSTM.I_CODE = O.I_CODE 
			AND INSTM.A_TYPE = O.A_TYPE 
			AND INSTM.M_TYPE = O.M_TYPE 
		JOIN TTRD_ACC_SECU ACCS 
			ON ACCS.ACCID = O.SECU_ACCT_ID 
		LEFT JOIN TTRD_P_TYPE PT 
			ON PT.P_TYPE = INSTM.P_TYPE 
		LEFT JOIN TTRD_INSTITUTION INSTU 
			ON INSTU.I_ID = INSTM.PARTY_ID 
		WHERE L.PRINT_NO = ? AND E.INST_ID = -1 
	)
) D]]></xml-property>
