                    <property name="nativeName">P_TYPE_NAME</property>
                    <property name="dataType">string</property>
                </structure>
                <structure>
                    <property name="position">3</property>
                    <property name="name">P_TYPE_GRP</property>
                    <property name="nativeName">P_TYPE_GRP</property>
                    <property name="dataType">string</property>
                </structure>
                <structure>
                    <property name="position">4</property>
                    <property name="name">SUBJ_CODE</property>
                    <property name="nativeName">SUBJ_CODE</property>
                    <property name="dataType">string</property>
                </structure>
                <structure>
                    <property name="position">5</property>
                    <property name="name">CORE_ACCT_CODE</property>
                    <property name="nativeName">CORE_ACCT_CODE</property>
                    <property name="dataType">string</property>
                </structure>
                <structure>
                    <property name="position">6</property>
                    <property name="name">CORE_ACCT_NAME</property>
                    <property name="nativeName">CORE_ACCT_NAME</property>
                    <property name="dataType">string</property>
                </structure>
                <structure>
                    <property name="position">7</property>
                    <property name="name">VALUE</property>
                    <property name="nativeName">VALUE</property>
                    <property name="dataType">decimal</property>
                </structure>
                <structure>
                    <property name="position">8</property>
                    <property name="name">MIN_STEP</property>
                    <property name="nativeName">MIN_STEP</property>
                    <property name="dataType">decimal</property>
                </structure>
                <structure>
                    <property name="position">9</property>
                    <property name="name">AVG_STEP</property>
                    <property name="nativeName">AVG_STEP</property>
                    <property name="dataType">decimal</property>
                </structure>
                <structure>
                    <property name="position">10</property>
                    <property name="name">STEP_NAME</property>
                    <property name="nativeName">STEP_NAME</property>
                    <property name="dataType">string</property>
                </structure>
            </list-property>
            <xml-property name="queryText"><![CDATA[--CHG_ID关联记账变动，找到ACCTG_OBJ_ID对应的核算对象
--MIN_STEP取最小步骤，与AVG_STEP比较，相等则取最小步骤的名称
SELECT 
	G.*, 
	DICT.DICT_VALUE AS STEP_NAME 
FROM (
	SELECT 
		INNER_ACCT_SN, 
		P_TYPE_NAME, 
		P_TYPE_GRP, 
		SUBJ_CODE, 
		CORE_ACCT_CODE, 
		CORE_ACCT_NAME, 
		SUM((DEBIT_VALUE - CREDIT_VALUE) * RED_BLUE_DIR) AS VALUE, 
		MIN(STEP) AS MIN_STEP, 
		AVG(STEP) AS AVG_STEP 
	FROM (
		SELECT 
			INNER_ACCT_SN, 
			P_TYPE_NAME, 
			SUBJ_CODE, 
			CORE_ACCT_CODE, 
			CORE_ACCT_NAME, 
			DEBIT_VALUE, 
			CREDIT_VALUE, 
			RED_BLUE_DIR, 
			TO_NUMBER(STEP) AS STEP, 
			CASE WHEN P_TYPE = '0121' THEN 
				'0121' 
			WHEN P_TYPE = '0134' AND (T_TYPE = '银行系统内-省联社' OR T_TYPE = '银行系统内-其他联社') THEN 
				'0134_1'  
			WHEN P_TYPE = '0134' AND (T_TYPE = '银行系统外') THEN 
				'0134_2' 
			WHEN P_TYPE = '0134' AND T_TYPE <> '银行系统内-省联社' AND T_TYPE <> '银行系统内-其他联社' AND T_TYPE <>'银行系统外' THEN 
				'0134_3'
			WHEN P_TYPE = '0100' AND P_CLASS = '保本理财' THEN 
				'0100_1' 
			WHEN P_TYPE = '0100' AND P_CLASS = '买入返售其他金融资产' THEN 
				'0100_2' 
			WHEN P_TYPE = '0100' AND P_CLASS <> '保本理财' AND P_CLASS <> '买入返售其他金融资产' THEN 
				'0100_3' 
			WHEN P_TYPE = '0000' AND ACCTYPE_FLAG = 0 THEN 
				'0000_1' 
			WHEN P_TYPE = '0000' AND ACCTYPE_FLAG > 0 THEN 
				'0000_2' 
			END AS P_TYPE_GRP  
		FROM (
			SELECT 
				E.ENTRY_ID, 
				E.FLOW_ID, 
				E.CORE_ACCT_CODE, 
				E.CORE_ACCT_NAME, 
				E.SUBJ_CODE, 
				E.INNER_ACCT_SN, 
				E.DEBIT_CREDIT_FLAG, 
				E.RED_BLUE_FLAG, 
				E.STEP, 
				CASE WHEN E.DEBIT_CREDIT_FLAG = '1' THEN 
					VALUE 
				ELSE 0 END DEBIT_VALUE,
				CASE WHEN E.DEBIT_CREDIT_FLAG = '2' THEN 
					VALUE 
				ELSE 0 END CREDIT_VALUE, 
				CASE WHEN E.RED_BLUE_FLAG = '2' THEN 
					-1 
				ELSE 1 END AS RED_BLUE_DIR, 
				INSTM.P_TYPE, 
				PT.P_TYPE_NAME, 
				INSTM.P_CLASS, 
				INSTR((SELECT ','||P_VALUE||',' FROM TPARA WHERE P_CODE='BONDRIGHTACCOUNTYPES'),','||ACCS.ACTING_TYPE||',') AS ACCTYPE_FLAG, 
				SUBSTR(INSTU.T_PATH, 0, INSTR(INSTU.T_PATH, '.') - 1) AS T_TYPE 
			FROM TTRD_BOOKKEEPING_ENTRY_HIS E
			JOIN TTRD_SETTLE_PRINT_LOG L 
				ON L.FLOW_ID = E.FLOW_ID 
			JOIN TTRD_BOOKKEEPING_SECU_CHG_HIS C 
				ON C.CHG_ID = E.CHG_ID 
			JOIN TTRD_ACCOUNTING_SECU_OBJ_HIS O 
				ON O.OBJ_ID = C.ACCTG_OBJ_ID 
				AND O.BEG_DATE = C.CHG_DATE 
			JOIN TTRD_INSTRUMENT INSTM 
				ON INSTM.I_CODE = O.I_CODE 
				AND INSTM.A_TYPE = O.A_TYPE 
				AND INSTM.M_TYPE = O.M_TYPE 
			JOIN TTRD_ACC_SECU ACCS 
				ON ACCS.ACCID = O.SECU_ACCT_ID 
			LEFT JOIN TTRD_P_TYPE PT 
				ON PT.P_TYPE = INSTM.P_TYPE 
			LEFT JOIN TTRD_INSTITUTION INSTU 
				ON INSTU.I_ID = INSTM.PARTY_ID 
			WHERE L.PRINT_NO = ? AND E.INST_ID = -1 
		)
	) 
	GROUP BY INNER_ACCT_SN, P_TYPE_NAME, P_TYPE_GRP, SUBJ_CODE, CORE_ACCT_CODE, CORE_ACCT_NAME 
) G 
LEFT JOIN TTRD_DICT_MULT_LANG DICT 
	ON DICT.DICT_SUB_TYPE = 'bkpStep' AND DICT.DICT_KEY = TO_CHAR(G.MIN_STEP) 
ORDER BY INNER_ACCT_SN, P_TYPE_GRP, VALUE DESC ]]></xml-property>
