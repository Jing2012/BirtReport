            <xml-property name="queryText"><![CDATA[--查询日期从开始日前一天开始
WITH ACCOUNTING_SECU_OBJ AS (
	SELECT
		ASOBJ.BEG_DATE, 
		ASOBJ.OBJ_ID, 
		ASOBJ.TRADE_ID, 
		ASOBJ.SECU_ACCT_ID, 
		(ASOBJ.REAL_AMOUNT + ASOBJ.DUE_AMOUNT) AS REAL_AMOUNT, 
		ASOBJ.AI, 
		ASOBJ.DUE_AI, 
		ASOBJ.PRFT_IR_AI AS PRFT_IR, 
		ASOBJ.EXTRA_DIM, 
		ASOBJ.AI_COST, 
		INSTM.I_CODE, INSTM.A_TYPE, INSTM.M_TYPE, 
		INSTM.I_NAME, 
		INSTM.P_CLASS, 
		TRD.SETDATE, 
		TRD.BND_AIAMOUNT AS TRD_AI_COST, 
		ACCS.ACCNAME 
	FROM TTRD_ACCOUNTING_SECU_OBJ ASOBJ 
	JOIN TTRD_ACC_SECU ACCS
		ON ACCS.ACCID = ASOBJ.SECU_ACCT_ID 
	JOIN TTRD_INSTRUMENT INSTM
		ON INSTM.I_CODE = ASOBJ.I_CODE 
		AND INSTM.A_TYPE = ASOBJ.A_TYPE 
		AND INSTM.M_TYPE = ASOBJ.M_TYPE 
	JOIN TTRD_OTC_TRADE TRD 
		ON TRD.INTORDID = ASOBJ.TRADE_ID 
	WHERE ACCS.I_ID = ? 
		AND ASOBJ.BEG_DATE >= ? AND ASOBJ.BEG_DATE <= ? 
		AND INSTM.P_TYPE NOT IN ('0141') 
		AND EXISTS(
			SELECT 1 FROM V_ACCAUTH_USER_NODE_MAP M 
			WHERE M.USER_ID = ? AND M.SUPER_NODE_ID = ? 
				AND M.COMPONENTTYPE = '0' AND M.V_POPEDOM >= 1
				AND M.COMPONENT = ASOBJ.SECU_ACCT_ID 
		)
	UNION ALL 
	SELECT 
		ASOBJ.BEG_DATE, 
		ASOBJ.OBJ_ID, 
		ASOBJ.TRADE_ID, 
		ASOBJ.SECU_ACCT_ID, 
		(ASOBJ.REAL_AMOUNT + ASOBJ.DUE_AMOUNT) AS REAL_AMOUNT, 
		ASOBJ.AI, 
		ASOBJ.DUE_AI, 
		ASOBJ.PRFT_IR_AI AS PRFT_IR, 
		ASOBJ.EXTRA_DIM, 
		ASOBJ.AI_COST, 
		INSTM.I_CODE, INSTM.A_TYPE, INSTM.M_TYPE, 
		INSTM.I_NAME, 
		INSTM.P_CLASS, 
		TRD.SETDATE, 
		TRD.BND_AIAMOUNT AS TRD_AI_COST, 
		ACCS.ACCNAME 
	FROM TTRD_ACCOUNTING_SECU_OBJ_HIS ASOBJ
	JOIN TTRD_ACC_SECU ACCS
		ON ACCS.ACCID = ASOBJ.SECU_ACCT_ID 
	JOIN TTRD_INSTRUMENT INSTM
		ON INSTM.I_CODE = ASOBJ.I_CODE 
		AND INSTM.A_TYPE = ASOBJ.A_TYPE 
		AND INSTM.M_TYPE = ASOBJ.M_TYPE 
	JOIN TTRD_OTC_TRADE TRD 
		ON TRD.INTORDID = ASOBJ.TRADE_ID 
	WHERE ACCS.I_ID = ? 
		AND ASOBJ.BEG_DATE >= ? AND ASOBJ.BEG_DATE <= ? 
		AND INSTM.P_TYPE NOT IN ('0141') 
		AND EXISTS(
			SELECT 1 FROM V_ACCAUTH_USER_NODE_MAP M 
			WHERE M.USER_ID = ? AND M.SUPER_NODE_ID = ? 
				AND M.COMPONENTTYPE = '0' AND M.V_POPEDOM >= 1
				AND M.COMPONENT = ASOBJ.SECU_ACCT_ID 
		)
) 
SELECT 
	GD.* 
FROM (
	SELECT 
		GOBJ.*, 
		CASE WHEN GOBJ.MIN_DATE = ? THEN 
			MINOBJ.PRFT_IR 
		ELSE 0 END AS MIN_PRFT_IR, 
		CASE WHEN GOBJ.MIN_DATE = ? THEN 
			MINOBJ.AI 
		ELSE 0 END AS MIN_AI, 
		CASE WHEN GOBJ.MIN_DATE = ? THEN 
			MINOBJ.DUE_AI 
		ELSE 0 END AS MIN_DUE_AI, 
		CASE WHEN GOBJ.MIN_DATE = ? THEN 
			MINOBJ.AI_COST 
		ELSE 0 END AS MIN_AI_COST, 
		MAXOBJ.REAL_AMOUNT AS MAX_REAL_AMOUNT, 
		MAXOBJ.PRFT_IR AS MAX_PRFT_IR, 
		MAXOBJ.AI AS MAX_AI, 
		MAXOBJ.DUE_AI AS MAX_DUE_AI, 
		MAXOBJ.AI_COST AS MAX_AI_COST, 
		COBJ.VCOUNT 
	FROM (
		SELECT 
			MIN(BEG_DATE) AS MIN_DATE, 
			MAX(BEG_DATE) AS MAX_DATE, 
			OBJ_ID, 
			TRADE_ID, 
			SECU_ACCT_ID, 
			I_CODE, 
			A_TYPE, 
			M_TYPE, 
			I_NAME, 
			P_CLASS, 
			SETDATE, 
			TRD_AI_COST, 
			ACCNAME, 
			EXTRA_DIM 
		FROM ACCOUNTING_SECU_OBJ 
		GROUP BY OBJ_ID, TRADE_ID, SECU_ACCT_ID, I_CODE, A_TYPE, M_TYPE, 
			I_NAME, P_CLASS, SETDATE, TRD_AI_COST, ACCNAME, EXTRA_DIM 
	) GOBJ 
	JOIN ACCOUNTING_SECU_OBJ MINOBJ 
		ON MINOBJ.OBJ_ID = GOBJ.OBJ_ID 
		AND MINOBJ.BEG_DATE = GOBJ.MIN_DATE 
	JOIN ACCOUNTING_SECU_OBJ MAXOBJ 
		ON MAXOBJ.OBJ_ID = GOBJ.OBJ_ID 
		AND MAXOBJ.BEG_DATE = GOBJ.MAX_DATE 
	LEFT JOIN (
		SELECT 
			OBJ_ID, 
			COUNT(OBJ_ID) AS VCOUNT 
		FROM ACCOUNTING_SECU_OBJ 
		WHERE ABS(REAL_AMOUNT) >= 0.01 OR ABS(AI) >= 0.01 OR ABS(DUE_AI) >= 0.01 
		GROUP BY OBJ_ID 
	) COBJ 
		ON COBJ.OBJ_ID = GOBJ.OBJ_ID 
) GD 
WHERE GD.VCOUNT > 0 OR ABS(MAX_PRFT_IR - MIN_PRFT_IR) >= 0.01
ORDER BY GD.SETDATE DESC ]]></xml-property>
