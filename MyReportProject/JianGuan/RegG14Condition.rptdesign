            <xml-property name="queryText"><![CDATA[WITH ACCOUNTING_OBJ AS (SELECT BEG_DATE,
      ISSUER_ID,
      T_PATH,
      T_CODE,
      sum(decode(P_CLASS, '同业拆借拆出', COST, null)) "TYCJ" , 
      sum(decode(P_CLASS, '同业借款借出', COST, null)) "TYJK" ,    
      sum(decode(P_CLASS, '存放同业定期', COST, null)) "CFTY" ,
      sum(decode(P_CLASS, '国债', COST,'地方政府债',COST,'央行票据',COST,'政策性银行普通金融债',COST,
      '政策性银行混合资本债',COST,'政策性银行次级债',COST,'商业银行普通金融债券',COST,'商业银行混合资本债',COST,'商业银行次级债',COST,'保险公司债',COST,'证券公司债',COST,'证券公司短期融资券',COST,'企业债',COST,'一般公司债',COST,'短期融资券',COST,'中期票据'
      ,COST,'可转债',COST,'可分离转债存债',COST,'可交换债',COST,'定向工具',COST,'资产支持证券',COST,'国际机构债券',COST,'私募债',COST,'其它金融机构债',COST,'政府支持机构债',COST,'其他',COST, null)) "BD" ,
      sum(decode(P_CLASS, '同业存单',COST,'保证收益型理财产品',COST,'保本浮动收益型理财产品',COST,'非保本浮动收益型理财产品',COST,'信托产品',COST,'信托受益权',COST,'信贷资产',COST,'保本固定收益凭证'
      ,COST,'资管计划收益权',COST,'资产管理计划',COST,'信贷资产流转',COST,'应收融资租赁款',COST,'其他金融资产',COST, null)) "TDMDZT" ,
      sum(decode(P_CLASS, '债券质押式逆回购', COST,'债券买断式逆回购',COST, null)) "MRFS"
FROM       (SELECT C.BEG_DATE,
       BOND.ISSUER_ID,
       INSTITUTION.I_FULLNAME,
       INSTITUTION.T_CODE,
       INSTITUTION.I_LR_INST_CODE,
       SUBSTR(INSTITUTION.T_PATH, 1, INSTR(INSTITUTION.T_PATH, '.') - 1) AS T_PATH,
       BOND.P_CLASS,
       ABS(C.REAL_CP+C.DUE_CP)/10000 AS COST 
        FROM  (SELECT *
          FROM TTRD_ACCOUNTING_SECU_OBJ
         WHERE A_TYPE IN ('SPT_BD',
                          'SPT_ABS')
        UNION
        SELECT *
          FROM TTRD_ACCOUNTING_SECU_OBJ_HIS
         WHERE A_TYPE IN ('SPT_BD',
                          'SPT_ABS')) C
  INNER JOIN (SELECT A.ACCID, A.ACCNAME, A.ACTING_TYPE, B.TYPENAME
               FROM TTRD_ACC_SECU A
               LEFT JOIN TTRD_ACCTG_ACCOUNT_TYPE B
                 ON A.ACTING_TYPE = B.TYPEID
              WHERE A.ACTING_TYPE IN  ('1','2','3')
             /*交易性金融资产*/
             ) AA
    ON C.SECU_ACCT_ID = AA.ACCID
  LEFT JOIN TBND BOND
    ON C.I_CODE = BOND.I_CODE
  LEFT JOIN TTRD_INSTITUTION INSTITUTION
    ON BOND.ISSUER_ID=INSTITUTION.I_ID 
    WHERE C.REAL_AMOUNT<>0 
    
    UNION ALL
    SELECT C.BEG_DATE,
       TRADE.PARTYID,
       INSTITUTION.I_FULLNAME,
       INSTITUTION.T_CODE,
       INSTITUTION.I_LR_INST_CODE,
       SUBSTR(INSTITUTION.T_PATH, 1, INSTR(INSTITUTION.T_PATH, '.') - 1) AS T_PATH,
       CASHLB.P_CLASS,
       ABS(C.REAL_CP+C.DUE_CP)/10000 AS COST           
      FROM (SELECT *
          FROM TTRD_ACCOUNTING_SECU_OBJ
         WHERE A_TYPE ='SPT_LBS'
        UNION
        SELECT *
          FROM TTRD_ACCOUNTING_SECU_OBJ_HIS
         WHERE A_TYPE ='SPT_LBS') C
  INNER JOIN (SELECT A.ACCID, A.ACCNAME, A.ACTING_TYPE, B.TYPENAME
               FROM TTRD_ACC_SECU A
               LEFT JOIN TTRD_ACCTG_ACCOUNT_TYPE B
                 ON A.ACTING_TYPE = B.TYPEID
              WHERE A.ACTING_TYPE = '116'
             /*其他类*/
             ) AA
    ON C.SECU_ACCT_ID = AA.ACCID
  LEFT JOIN TTRD_CASHLB CASHLB
    ON C.I_CODE = CASHLB.I_CODE
  LEFT JOIN TTRD_OTC_TRADE TRADE 
  ON C.TRADE_ID=TRADE.INTORDID
  LEFT JOIN TTRD_INSTITUTION INSTITUTION
    ON TRADE.PARTYID=INSTITUTION.I_ID 
   WHERE TRADE.TRDTYPE IN ('0121100','0122100','0125100','0123100') AND C.REAL_AMOUNT<>0
   
   UNION ALL
   SELECT C.BEG_DATE,
       TRADE.PARTYID,
       INSTITUTION.I_FULLNAME,
       INSTITUTION.T_CODE,
       INSTITUTION.I_LR_INST_CODE,
       SUBSTR(INSTITUTION.T_PATH, 1, INSTR(INSTITUTION.T_PATH, '.') - 1) AS T_PATH,
       CASHLB.P_CLASS,
       ABS(C.REAL_CP+C.DUE_CP)/10000 AS COST      
   FROM (SELECT *
          FROM TTRD_ACCOUNTING_SECU_OBJ
         WHERE A_TYPE ='SPT_LBS'
        UNION ALL
        SELECT *
          FROM TTRD_ACCOUNTING_SECU_OBJ_HIS
         WHERE A_TYPE ='SPT_LBS') C
  INNER JOIN (SELECT A.ACCID, A.ACCNAME, A.ACTING_TYPE, B.TYPENAME
               FROM TTRD_ACC_SECU A
               LEFT JOIN TTRD_ACCTG_ACCOUNT_TYPE B
                 ON A.ACTING_TYPE = B.TYPEID
              WHERE A.ACTING_TYPE IN  ('1','2','3','4')
             ) AA
    ON C.SECU_ACCT_ID = AA.ACCID
  LEFT JOIN TTRD_CASHLB CASHLB
    ON C.I_CODE = CASHLB.I_CODE
  LEFT JOIN TTRD_OTC_TRADE TRADE 
  ON C.TRADE_ID=TRADE.INTORDID
  LEFT JOIN TTRD_INSTITUTION INSTITUTION
    ON TRADE.PARTYID=INSTITUTION.I_ID  
    WHERE C.REAL_AMOUNT<>0 
       UNION ALL
    SELECT C.BEG_DATE,
       TRADE.PARTYID,
       INSTITUTION.I_FULLNAME,
       INSTITUTION.T_CODE,
       INSTITUTION.I_LR_INST_CODE,
       SUBSTR(INSTITUTION.T_PATH, 1, INSTR(INSTITUTION.T_PATH, '.') - 1) AS T_PATH,
       OUTRIGHT.P_CLASS,
       ABS(C.REAL_CP+C.DUE_CP)/10000 AS COST           
      FROM (SELECT *
          FROM TTRD_ACCOUNTING_SECU_OBJ
        UNION
        SELECT *
          FROM TTRD_ACCOUNTING_SECU_OBJ_HIS) C
  INNER JOIN (SELECT A.ACCID, A.ACCNAME, A.ACTING_TYPE, B.TYPENAME
               FROM TTRD_ACC_SECU A
               LEFT JOIN TTRD_ACCTG_ACCOUNT_TYPE B
                 ON A.ACTING_TYPE = B.TYPEID
              WHERE A.ACTING_TYPE = '297'
             /*其他类*/
             ) AA
    ON C.SECU_ACCT_ID = AA.ACCID
  LEFT JOIN TTRD_OUTRIGHT OUTRIGHT
    ON C.I_CODE = OUTRIGHT.I_CODE
  LEFT JOIN TTRD_OTC_TRADE TRADE 
  ON C.TRADE_ID=TRADE.INTORDID
  LEFT JOIN TTRD_INSTITUTION INSTITUTION
    ON TRADE.PARTYID=INSTITUTION.I_ID 
   WHERE TRADE.TRDTYPE IN ('0220100') AND C.REAL_AMOUNT<>0) 
    GROUP BY BEG_DATE,
      ISSUER_ID,
      T_CODE,
      T_PATH)
    SELECT OBJ.*,NODE.NODE_NAME,INSTITUTION.I_LR_INST_CODE FROM ACCOUNTING_OBJ OBJ
      LEFT JOIN TREE_NODE_INFO NODE 
      ON OBJ.T_CODE=NODE.NODE_ID
      LEFT JOIN TTRD_INSTITUTION INSTITUTION
      ON NODE.NODE_NAME=INSTITUTION.I_FULLNAME
       WHERE OBJ.BEG_DATE=? AND 
          OBJ.T_PATH NOT IN ('境内非金融企业','境外非金融企业') ]]></xml-property>
        </oda-data-set>
    </data-sets>
