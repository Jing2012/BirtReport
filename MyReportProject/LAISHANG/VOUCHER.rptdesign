            <xml-property name="queryText"><![CDATA[with set_instruction as (
select gtsi.inst_id,
       gtsi.trd_type,
       tsis.secu_inst_id as secu_cash_inst_id,
       gtsi.h_i_code,
       gtsi.h_a_type,
       gtsi.h_m_type,
       gtsi.operator_name
  from (select * from ttrd_set_instruction
        union
        select * from ttrd_set_instruction_his) gtsi
 inner join (select * from ttrd_set_instruction
             union
             select * from ttrd_set_instruction_his) tsi on tsi.inst_grp_id = gtsi.inst_id
 inner join (select * from ttrd_set_instruction_secu
             union all
             select * from ttrd_set_instruction_secu_his) tsis on tsis.inst_id = tsi.inst_id or tsis.inst_id = gtsi.inst_id
 where gtsi.inst_id = ?
union all
select gtsi.inst_id,
       gtsi.trd_type,
       tsic.cash_inst_id as secu_cash_inst_id,
       gtsi.h_i_code,
       gtsi.h_a_type,
       gtsi.h_m_type,
       gtsi.operator_name
  from (select * from ttrd_set_instruction
        union
        select * from ttrd_set_instruction_his) gtsi
 inner join (select * from ttrd_set_instruction
             union
             select * from ttrd_set_instruction_his) tsi on tsi.inst_grp_id = gtsi.inst_id
 inner join (select * from ttrd_set_instruction_cash
             union
             select * from ttrd_set_instruction_cash_his) tsic on tsic.inst_id = tsi.inst_id or tsic.inst_id = gtsi.inst_id
 where gtsi.inst_id = ?
)
select distinct trf.sendflow_no,
       be.flow_id,
       trf.is_erase,
       be.bkkpg_org_id,
       trdtype.description as trdtype_name,
       inst.h_i_code,
       ti.i_name,
       replace(taml.send_time,'-',' /') as send_time,
       inst.operator_name,
       (select account
          from ttrd_auth_user
         where user_id = decode(trf.is_erase, 1, tbsm.set_user_id, 2, tbsm.era_user_id)) as set_era_user_account,
       (select user_name
          from ttrd_auth_user
         where user_id = decode(trf.is_erase, 1, tbsm.set_user_id, 2, tbsm.era_user_id)) as set_era_user_name,
       (select user_name
          from ttrd_auth_user
         where user_id = decode(trf.is_erase, 2, tbsm.auth_era_user_id, null)) as auth_era_user_name
  from (select (select distinct flow_id
                  from ttrd_bookkeeping_entry
                 where tsk_id = bec.tsk_id
                   and grp_flow_id = bec.flow_id) as ref_flow_id,bec.* from ttrd_bookkeeping_entry bec
        union all
        select (select distinct flow_id
                  from ttrd_bookkeeping_entry_his
                 where tsk_id = beh.tsk_id
                   and grp_flow_id = beh.flow_id) as ref_flow_id,beh.* from ttrd_bookkeeping_entry_his beh) be
 inner join set_instruction inst on inst.secu_cash_inst_id = be.inst_id
 inner join ttrd_record_flowmapping trf on trf.is_erase in (1, 2)
                                       and trf.state = 0
                                       and trf.flow_no = be.flow_id
  left join ttrd_trade_type trdtype on trdtype.trd_type = inst.trd_type
  left join ttrd_instrument ti on ti.i_code = inst.h_i_code
                              and ti.a_type = inst.h_a_type
                              and ti.m_type = inst.h_m_type
  left join ttrd_bkp_settle_mapping tbsm on tbsm.flow_id = nvl(be.ref_flow_id, be.flow_id)
  left join ttrd_aidsys_msg_log taml on taml.send_no = trf.sendflow_no
                                    and (dbms_lob.instr(taml.send_content, utl_raw.cast_to_raw('<service_id>00310000BATCH0</service_id>'), 1, 1) > 0
                                         or dbms_lob.instr(taml.send_content, utl_raw.cast_to_raw('<service_id>00310000REVER0</service_id>'), 1, 1) > 0)
 where be.grp_flow_id = '-1'
   and be.estd_or_real = 'R'
   and be.state in ('1', '3')
 order by trf.sendflow_no, be.flow_id
]]></xml-property>
