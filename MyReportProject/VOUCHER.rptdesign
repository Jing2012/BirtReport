            <xml-property name="queryText"><![CDATA[WITH CTE_SECU_CASH_INST_ID AS (
	SELECT 
		DISTINCT 
		DECODE(SECU_INST_SETGRP_ID, NULL, SECU_INST_ID, '', SECU_INST_ID, '-1', SECU_INST_ID, '0', SECU_INST_ID, SECU_INST_SETGRP_ID) AS INST_ID, 
		GM.TRD_TYPE, 
		GM.H_I_CODE, 
		GM.H_A_TYPE, 
		GM.H_M_TYPE, 
		GM.OPERATOR_NAME 
	FROM (
		SELECT * FROM TTRD_SET_INSTRUCTION_SECU 
		UNION ALL 
		SELECT * FROM TTRD_SET_INSTRUCTION_SECU_HIS 
	) SECU, (
		SELECT * FROM TTRD_SET_INSTRUCTION 
		UNION ALL 
		SELECT * FROM TTRD_SET_INSTRUCTION_HIS 
	) M, (
		SELECT * FROM TTRD_SET_INSTRUCTION 
		UNION ALL 
		SELECT * FROM TTRD_SET_INSTRUCTION_HIS 
	) GM 
	WHERE GM.INST_ID = ? 
		AND SECU.INST_ID = M.INST_ID 
		AND M.INST_GRP_ID = GM.INST_ID 
	UNION ALL 
	SELECT 
		DISTINCT 
		DECODE(CASH_INST_SETGRP_ID, NULL, CASH_INST_ID, '', CASH_INST_ID, '-1', CASH_INST_ID, '0', CASH_INST_ID, CASH_INST_SETGRP_ID) AS INST_ID, 
		GM.TRD_TYPE, 
		GM.H_I_CODE, 
		GM.H_A_TYPE, 
		GM.H_M_TYPE, 
		GM.OPERATOR_NAME 
	FROM (
		SELECT * FROM TTRD_SET_INSTRUCTION_CASH 
		UNION ALL 
		SELECT * FROM TTRD_SET_INSTRUCTION_CASH_HIS
	) CASH, (
		SELECT * FROM TTRD_SET_INSTRUCTION 
		UNION ALL 
		SELECT * FROM TTRD_SET_INSTRUCTION_HIS 
	) M, (
		SELECT * FROM TTRD_SET_INSTRUCTION 
		UNION ALL 
		SELECT * FROM TTRD_SET_INSTRUCTION_HIS 
	) GM 
	WHERE GM.INST_ID = ? 
		AND CASH.INST_ID = M.INST_ID 
		AND M.INST_GRP_ID = GM.INST_ID 
) 


SELECT 
	DISTINCT 
	FLOW_ID, 
	HOSTFLOW_NO, 
	IS_ERASE, 
	ENTRY_DATE, 
	BKKPG_ORG_ID, 
	H_I_CODE, 
	OPERATOR_NAME, 
	I_NAME, 
	TRD_TYPE, 
	TRDTYPE_NAME, 
	SEND_TIME, 
	SET_ERA_USER_ACCOUNT, 
	SET_ERA_USER_NAME, 
	AUTH_ERA_USER_NAME,
  i_fullNAME
FROM (
	SELECT 
		E.FLOW_ID, 
		E.ENTRY_DATE, 
		E.BKKPG_ORG_ID, 
		I.H_I_CODE, 
		I.OPERATOR_NAME, 
		INSTM.I_NAME, 
		I.TRD_TYPE, 
		TRDTYPE.DESCRIPTION AS TRDTYPE_NAME, 
		NVL(MP.HOSTFLOW_NO, '') AS HOSTFLOW_NO, 
		NVL(MP.IS_ERASE, 1) AS IS_ERASE, 
		(
			SELECT 
				MAX(L.SEND_TIME) 
			FROM TTRD_AIDSYS_MSG_LOG L 
			WHERE L.SEND_NO = MP.SENDFLOW_NO 
		) AS SEND_TIME, 
		(
			SELECT ACCOUNT 
			FROM TTRD_AUTH_USER 
			WHERE USER_ID = DECODE(MP.IS_ERASE, 1, BSM.SET_USER_ID, 2, BSM.ERA_USER_ID)
		) AS SET_ERA_USER_ACCOUNT,
		(
			SELECT USER_NAME 
			FROM TTRD_AUTH_USER 
			WHERE USER_ID = DECODE(MP.IS_ERASE, 1, BSM.SET_USER_ID, 2, BSM.ERA_USER_ID)
		) AS SET_ERA_USER_NAME,
		(
			SELECT USER_NAME 
			FROM TTRD_AUTH_USER 
			WHERE USER_ID = DECODE(MP.IS_ERASE, 2, BSM.AUTH_ERA_USER_ID, NULL)
		) AS AUTH_ERA_USER_NAME ,
      y.i_fullNAME
	FROM (
		SELECT * FROM TTRD_BOOKKEEPING_ENTRY 
		UNION 
		SELECT * FROM TTRD_BOOKKEEPING_ENTRY_HIS 
	) E 
	JOIN CTE_SECU_CASH_INST_ID I 
		ON I.INST_ID = E.INST_ID 
	LEFT JOIN TTRD_TRADE_TYPE TRDTYPE 
		ON TRDTYPE.TRD_TYPE = I.TRD_TYPE 
	LEFT JOIN TTRD_INSTRUMENT INSTM 
		ON INSTM.I_CODE = I.H_I_CODE 
		AND INSTM.A_TYPE = I.H_A_TYPE 
		AND INSTM.M_TYPE = I.H_M_TYPE 
	LEFT JOIN (
		SELECT 
			SENDFLOW_NO, FLOW_NO, HOSTFLOW_NO, IS_ERASE, HIS_FLAG 
		FROM TTRD_RECORD_FLOWMAPPING 
		WHERE STATE = 0 AND HIS_FLAG = 0 
	  UNION 
    SELECT 
      SENDFLOW_NO, FLOW_NO, HOSTFLOW_NO, IS_ERASE, HIS_FLAG 
    FROM TTRD_RECORD_FLOWMAPPING_HIS 
    WHERE STATE = 0 AND HIS_FLAG = 0 
  ) MP 
    ON MP.FLOW_NO = E.FLOW_ID 
  LEFT JOIN TTRD_BKP_SETTLE_MAPPING BSM 
    ON BSM.FLOW_ID = E.FLOW_ID 
  left join TTRD_INSTITUTION y 
  on y.org_id = E.bkkpg_org_id
  WHERE E.STATE = '1' AND E.GRP_FLOW_ID = '-1' 
  
  UNION 

  SELECT 
    E.FLOW_ID, 
    E.ENTRY_DATE, 
    E.BKKPG_ORG_ID, 
    I.H_I_CODE, 
    I.OPERATOR_NAME, 
    INSTM.I_NAME, 
    I.TRD_TYPE, 
    TRDTYPE.DESCRIPTION AS TRDTYPE_NAME, 
    MP.HOSTFLOW_NO, 
    MP.IS_ERASE, 
    (
      SELECT 
        MAX(L.SEND_TIME) 
      FROM TTRD_AIDSYS_MSG_LOG L 
      WHERE L.SEND_NO = MP.SENDFLOW_NO 
    ) AS SEND_TIME, 
    (
      SELECT ACCOUNT 
      FROM TTRD_AUTH_USER 
      WHERE USER_ID = DECODE(MP.IS_ERASE, 1, BSM.SET_USER_ID, 2, BSM.ERA_USER_ID)
    ) AS SET_ERA_USER_ACCOUNT,
    (
      SELECT USER_NAME 
      FROM TTRD_AUTH_USER 
      WHERE USER_ID = DECODE(MP.IS_ERASE, 1, BSM.SET_USER_ID, 2, BSM.ERA_USER_ID)
    ) AS SET_ERA_USER_NAME,
    (
      SELECT USER_NAME 
      FROM TTRD_AUTH_USER 
      WHERE USER_ID = DECODE(MP.IS_ERASE, 2, BSM.AUTH_ERA_USER_ID, NULL)
    ) AS AUTH_ERA_USER_NAME ,
      y.i_fullNAME
  FROM (
    SELECT * FROM TTRD_BOOKKEEPING_ENTRY 
    UNION 
    SELECT * FROM TTRD_BOOKKEEPING_ENTRY_HIS 
  ) E 
  JOIN CTE_SECU_CASH_INST_ID I 
    ON I.INST_ID = E.INST_ID 
  LEFT JOIN TTRD_TRADE_TYPE TRDTYPE 
    ON TRDTYPE.TRD_TYPE = I.TRD_TYPE 
  LEFT JOIN TTRD_INSTRUMENT INSTM 
    ON INSTM.I_CODE = I.H_I_CODE 
    AND INSTM.A_TYPE = I.H_A_TYPE 
    AND INSTM.M_TYPE = I.H_M_TYPE 
  JOIN (
    SELECT 
      SENDFLOW_NO, FLOW_NO, HOSTFLOW_NO, IS_ERASE, HIS_FLAG 
    FROM TTRD_RECORD_FLOWMAPPING 
    WHERE STATE = 0 
    UNION 
    SELECT 
      SENDFLOW_NO, FLOW_NO, HOSTFLOW_NO, IS_ERASE, HIS_FLAG 
    FROM TTRD_RECORD_FLOWMAPPING_HIS 
    WHERE STATE = 0 
  ) MP 
    ON MP.FLOW_NO = E.FLOW_ID 
  LEFT JOIN TTRD_BKP_SETTLE_MAPPING BSM 
    ON BSM.FLOW_ID = E.FLOW_ID 
  left join TTRD_INSTITUTION y 
  on y.org_id = E.bkkpg_org_id
  WHERE E.STATE = '3' AND E.GRP_FLOW_ID = '-1' 
) 
ORDER BY FLOW_ID, IS_ERASE ]]></xml-property>
